{
  "name": "ide",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "electron/main.cjs",
  "author": {
    "name": "Pavlo",
    "email": "deadswallow@gmail.com"
  },
  "license": "MIT",
  "homepage": "https://braintease.dev",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "generate-favicons": "node generate-favicons.js",
    "generate-app-icons": "node generate-app-icons.js",
    "ripple-asm": "tsx src/services/ripple-assembler/cli.ts",
    "build:wasm:rust-interpreter": "cd src/rust-bf && ./build-wasm.sh && cp ./pkg/* ../../public/wasm || true && cd ../..",
    "build:wasm:rust-assembler": "cd src/ripple-asm && ./build-wasm.sh || true && cd ../..",
    "build:wasm": "npm run build:wasm:rust-interpreter && npm run build:wasm:rust-assembler",
    "build:rust-assembler": "cd src/ripple-asm && cargo build --release  || true && cd ../..",
    "build:rust-interpreter": "cd src/rust-bf && cargo build --release  || true && cd ../..",
    "build:rust-macro-expander": "cd src/bf-macro-expander && cargo build --release  || true && cd ../..",
    "build:rbt": "cd rbt && cargo build --release  || true && cd ..",
    "build:rcc": "cargo build --release || true",
    "build:native": "npm run build:rust-assembler && npm run build:rust-interpreter && npm run build:rust-macro-expander && npm run build:rbt && npm run build:rcc",
    "electron": "electron .",
    "electron:dev": "NODE_ENV=development electron .",
    "dist": "npm run build && electron-builder",
    "dist:mac": "npm run build && electron-builder --mac",
    "dist:win": "npm run build && electron-builder --win --x64",
    "dist:linux": "npm run build && electron-builder --linux --x64",
    "dist:all": "npm run build && electron-builder -mwl --x64",
    "publish-downloads": "./scripts/publish-downloads.sh"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/vite": "^4.1.14",
    "@types/jszip": "^3.4.1",
    "clsx": "^2.1.1",
    "commander": "^14.0.1",
    "jszip": "^3.10.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "ripple": "^0.2.124",
    "rxjs": "^7.8.2",
    "tailwindcss": "^4.1.14"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "@vitejs/plugin-basic-ssl": "^2.1.0",
    "@vitejs/plugin-react": "^5.0.4",
    "electron": "^38.2.2",
    "electron-builder": "^26.0.12",
    "eslint": "^9.37.0",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-plugin-react-refresh": "^0.4.23",
    "favicons": "^7.2.0",
    "globals": "^16.4.0",
    "mime-types": "^3.0.1",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.0",
    "vite": "^7.1.9",
    "vite-plugin-top-level-await": "^1.6.0",
    "vite-plugin-wasm": "^3.5.0",
    "vitest": "^3.2.4"
  }
}
